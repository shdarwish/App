<!-- This page showes the results from search by range -->


<!-- Start users Heading Section -->
<div class='content'>
  <div class='button'></div>
  <div class="mainbody-section text-center">
    <div class="container">
      <div class="row">
        <div id="appHeading" class="section-title text-center">
          <h3>Search Result</h3>
        </div>
      </div>
<!--end of users heading section-->
      <div class="row">
        <div class="col-md-4 col-md-offset-4">
          
            <div class=" shadbox">
              <table class="table text-center" >

<% if @search.search_foods.empty? %>
	<p> No records found</p>
<% else %>
	<% @search.search_foods.each do |c| %>
	<br>

		<div class="div">

		<p><%= link_to c.name, food_path(c.id) %></p>
		</div>

	<% end %>
<% end %>
                </table>

            </div>
        </div>
      </div>
    </div>
  </div>
</div>